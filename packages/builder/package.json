{
  "name": "@budibase/builder",
  "version": "1.0.44-alpha.9",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "build": "routify -b && vite build --emptyOutDir",
    "start": "routify -c rollup",
    "test": "jest --maxWorkers=2",
    "test:coverage": "jest --coverage --maxWorkers=2",
    "test:watch": "jest --watchAll",
    "dev:builder": "routify -c dev:vite",
    "dev:vite": "vite --host 0.0.0.0",
    "rollup": "rollup -c -w",
    "cy:setup": "node ./cypress/setup.js",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:run:ci": "cypress run --record",
    "cy:test": "start-server-and-test cy:setup http://localhost:10001/builder cy:run",
    "cy:ci": "start-server-and-test cy:setup http://localhost:10001/builder cy:run",
    "cy:debug": "start-server-and-test cy:setup http://localhost:10001/builder cy:open"
  },
  "jest": {
    "globals": {
      "GLOBALS": {
        "client": "web"
      }
    },
    "testURL": "http://jest-breaks-if-this-does-not-exist",
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/src/$1",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "components(.*)$": "<rootDir>/src/components$1",
      "builderStore(.*)$": "<rootDir>/src/builderStore$1",
      "stores(.*)$": "<rootDir>/src/stores$1",
      "analytics(.*)$": "<rootDir>/src/analytics$1",
      "^@budibase/bbui$": "<rootDir>/node_modules/@budibase/bbui/src/index.js",
      "^@budibase/string-templates": "<rootDir>/node_modules/@budibase/string-templates/src/index.js"
    },
    "moduleFileExtensions": [
      "js",
      "mjs",
      "svelte",
      "css",
      "scss"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/node_module"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.mjs$": "babel-jest",
      "^.+\\.svelte$": "svelte-jester",
      ".+\\.(css|styl|less|sass|scss)$": "jest-css-modules-transform"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!@budibase|svelte-portal)"
      
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/cypress/"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "collectCoverageFrom": [
      "src/**/*.svelte"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:cypress/recommended"
    ],
    "rules": {
      "cypress/no-unnecessary-waiting": "off"
    }
  },
  "dependencies": {
    "@budibase/bbui": "^1.0.44-alpha.9",
    "@budibase/client": "^1.0.44-alpha.9",
    "@budibase/colorpicker": "1.1.2",
    "@budibase/string-templates": "^1.0.44-alpha.9",
    "@sentry/browser": "5.19.1",
    "@spectrum-css/page": "^3.0.1",
    "@spectrum-css/vars": "^3.0.1",
    "codemirror": "^5.59.0",
    "downloadjs": "1.4.7",
    "identity-obj-proxy": "^3.0.0",
    "lodash": "4.17.21",
    "posthog-js": "1.4.5",
    "remixicon": "2.5.0",
    "shortid": "2.2.15",
    "svelte-dnd-action": "^0.9.8",
    "svelte-htm": "^1.1.1",
    "svelte-loading-spinners": "^0.1.1",
    "svelte-portal": "0.1.0",
    "uuid": "8.3.1",
    "yup": "0.29.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.14",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/runtime": "^7.16.7",
    "@rollup/plugin-replace": "^2.4.2",
    "@roxi/routify": "2.18.0",
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.19",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/svelte": "^3.0.3",
    "@testing-library/user-event": "^13.5.0",
    "babel-jest": "^27.4.6",
    "cypress": "^5.1.0",
    "cypress-terminal-report": "^1.4.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-css-modules-transform": "^4.3.0",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.44.0",
    "rollup-plugin-copy": "^3.4.0",
    "start-server-and-test": "^1.12.1",
    "svelte": "^3.38.2",
    "svelte-inline-compile": "^0.1.0",
    "svelte-jester": "^2.1.5",
    "vite": "^2.1.5"
  },
  "gitHead": "115189f72a850bfb52b65ec61d932531bf327072"
}
